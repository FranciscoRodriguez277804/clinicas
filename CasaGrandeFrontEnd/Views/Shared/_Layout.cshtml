    <script src="https://cdnjs.cloudflare.com/ajax/libs/mdb-ui-kit/6.5.0/mdb.min.js"></script>
    <script>
        function mostrarPopup(mensaje, esExito) {
            var popup = $('#statusPopup');
            popup.removeClass('popup-success popup-error'); // Elimina clases anteriores
            popup.addClass(esExito ? 'popup-success' : 'popup-error'); // Añade la clase según el estado

            $('#statusMessage').text(mensaje); // Establece el mensaje
            popup.fadeIn(); // Muestra el popup con animación

            // Oculta el popup después de unos segundos
            setTimeout(function () {
                popup.fadeOut();
            }, 3000); // 3000 ms = 3 segundos
        }
    </script>
        

